{
  "version": 2,
  "builds": [
    {
      "src": "index.mjs",
      "use": "@vercel/node",
      "runtime":"Node.js",
      "includeFiles":[
        "resources/**/*"
      ]
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes":[
    {"src":"/(.*)", "dest":"index.mjs"}
  ]
}
