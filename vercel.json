{
  "version": 2,
  "builds": [
    {
      "src": "index.mjs",
      "use": "@vercel/node",
      "includeFiles": "resources/views/**",
      "runtime":"nodejs22.x"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes":[
    {"src":"/(.*)", "dest":"index.mjs"}
  ]
}
